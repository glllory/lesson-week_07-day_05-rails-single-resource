<h1>HELLO</h1>



<p><%= link_to 'create new movie', new_movie_path %></p>


 <% @movies.each do |movie| %>

<h3><%= movie.title %></h3>
<p><%= link_to 'View', movie_path(movie) %> </p>
<p><%= link_to 'Edit', edit_movie_path(movie) %> </p>
<p><%= link_to 'Delete',
 movie_path(movie),
 method: :delete, 
 data: {confirm: 'Are you sure you want to delete this movie?'}%></p>
<%end%>

